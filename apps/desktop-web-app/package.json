{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "deploy": "wrangler publish",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch --port 4200",
    "dev:css": "npm run generate:css -- --watch",
    "generate:css": "npx tailwindcss -i ./app/styles.css -o ./app/tailwind.css",
    "dev": "remix build && run-p \"dev:*\"",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.7.6",
    "@remix-run/cloudflare-workers": "^1.7.6",
    "@remix-run/react": "^1.7.6",
    "@supabase/supabase-js": "^2.1.1",
    "cross-env": "^7.0.3",
    "jwt-decode": "^3.1.2",
    "react": "^17.0.2",
    "react-aria": "^3.22.0",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.10"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@remix-run/dev": "^1.7.6",
    "@remix-run/eslint-config": "^1.7.6",
    "@types/react": "^17.0.52",
    "@types/react-dom": "^17.0.18",
    "eslint": "^8.27.0",
    "miniflare": "^2.11.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4",
    "wrangler": "^2.2.1"
  },
  "engines": {
    "node": ">=16.13"
  }
}
